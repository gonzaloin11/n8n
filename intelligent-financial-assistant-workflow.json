      "parameters": {
        "jsCode": "// Handle callback data\nconst item = $input.first().json;\nconst data = item.callback_query?.data || item.message?.text || '';\n\nlet action = '';\nlet rowId = '';\n\nif (data.startsWith('approve_')) {\n  action = 'approve';\n  rowId = data.replace('approve_', '');\n} else if (data.startsWith('reject_')) {\n  action = 'reject';\n  rowId = data.replace('reject_', '');\n} else if (data.startsWith('method_')) {\n  const parts = data.split('_');\n  action = 'payment';\n  rowId = parts[2] || '';\n  return [{\n    json: {\n      action,\n      rowId,\n      paymentMethod: parts[1] ? parts[1].charAt(0).toUpperCase() + parts[1].slice(1) : '',\n      chatId: item.callback_query?.message?.chat?.id || item.message?.chat?.id\n    }\n  }];\n}\n\nreturn [{\n  json: {\n    action,\n    rowId,\n    chatId: item.callback_query?.message?.chat?.id || item.message?.chat?.id\n  }\n}];"
      },
